    "STREAMDEVICE": {
        "R2-7-7": {
            "aliases": {
                "BASE": "EPICS_BASE"
            },
            "dependencies": [
                "ASYN",
                "BASE"
            ],
            "make-recipes": {
                "config": [
                    "mv -f $DIR/GNUmakefile $DIR/GNUmakefile.sumo.orig",
                    "for f in $$(find $DIR -name Makefile); do\\",
                    "  sed -i.sumo.orig -e 's#^\\(TOP *= *\\)\\.\\.\\(/\\.\\|\\)\\(.*\\)#\\1.\\3#' $$f;\\",
                    "done",
                    "cd $DIR && \\",
                    "echo | $(BASE)/bin/$(EPICS_HOST_ARCH)/makeBaseApp.pl \\",
                    "    -b $(BASE) -t support; \\",
                    "cp -a $DIR/RELEASE $DIR/configure"
                ],
                "distclean": [
                    "$(MAKE) -C $DIR clean",
                    "for d in bin configure dbd include lib; do\\",
                    "  rm -rf $DIR/$$d;\\",
                    "done",
                    "for f in $$(find $DIR -name '*.sumo.orig'); do\\",
                    "  cp -a $$f $$(dirname $$f)/$$(basename $$f .sumo.orig) && rm -f $$f;\\",
                    "done"
                ]
            },
            "releasefile": "RELEASE",
            "source": {
                "git": {
                    "tag": "stream_2_7_7",
                    "url": "https://github.com/paulscherrerinstitute/StreamDevice.git"
                }
            }
        }
    }
