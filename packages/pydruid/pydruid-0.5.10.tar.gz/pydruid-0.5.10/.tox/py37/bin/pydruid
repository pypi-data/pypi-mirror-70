#!/Users/max/code/pydruid/.tox/py37/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pydruid','console_scripts','pydruid'
__requires__ = 'pydruid'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pydruid', 'console_scripts', 'pydruid')()
    )
