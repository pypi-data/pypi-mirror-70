  vimPattern:
    - match: |-
        (?x) \b
        ( end
        | skip
        | start
        ) \b
      scope: support.function.vimPattern.viml
  vimStdPlugin:
    - match: |-
        (?x) \b
        ( Arguments
        | Break
        | Cfilter
        | Clear
        | Continue
        | DiffOrig
        | Evaluate
        | Finish
        | Gdb
        | Lfilter
        | Man
        | N
        | Next
        | Over
        | P
        | Print
        | Program
        | Run
        | Source
        | Step
        | Stop
        | S
        | TOhtml
        | TermdebugCommand
        | Termdebug
        | Winbar
        | XMLent
        | XMLns
        ) \b
      scope: support.class.stdplugin.viml
  vimSynCase:
    - match: |-
        (?x) \b
        ( ignore
        | match
        ) \b
      scope: support.function.vimSynCase.viml
  vimSynType:
    - match: |-
        (?x) \b
        ( case
        | clear
        | cluster
        | enable
        | include
        | iskeyword
        | keyword
        | list
        | manual
        | match
        | off
        | on
        | region
        | reset
        | sync
        ) \b
      scope: support.function.vimSynType.viml
  vimSyncC:
    - match: |-
        (?x) \b
        ( ccomment
        | clear
        | fromstart
        ) \b
      scope: support.function.vimSyncC.viml
  vimSyncLinecont:
    - match: |-
        (?x) \b
        ( linecont
        ) \b
      scope: support.function.vimSyncLinecont.viml
  vimSyncMatch:
    - match: |-
        (?x) \b
        ( match
        ) \b
      scope: support.function.vimSyncMatch.viml
  vimSyncNone:
    - match: |-
        (?x) \b
        ( NONE
        ) \b
      scope: support.function.vimSyncNone.viml
  vimSyncRegion:
    - match: |-
        (?x) \b
        ( region
        ) \b
      scope: support.function.vimSyncRegion.viml
  vimTodo:
    - match: |-
        (?x) \b
        ( COMBAK
        | FIXME
        | TODO
        | XXX
        ) \b
      scope: "support.constant.${1:/downcase}.viml"
  vimUserAttrbCmplt:
    - match: |-
        (?x) \b
        ( augroup
        | behave
        | buffer
        | color
        | command
        | compiler
        | cscope
        | customlist
        | custom
