%YAML 1.2
---
# http://www.sublimetext.com/docs/3/syntax.html
name: VimL
file_extensions:
  - vim
  - vimrc
  - gvimrc
  - nvimrc
  - _vimrc
  - vmb
first_line_match: |-
  (?x)

  	# Vimball
  	(?:^|\n)UseVimball\b

  	|

  	# Hashbang
  	^\#!.*(?:\s|\/|(?<=!)\b)
  		(?:vim|nvim)
  	(?:$|\s)

  	|

  	# Modeline
  	(?i:
  		# Vim/Vi modeline, accounting for all possible variations
  		(?:(?:\s|^)vi(?:m[<=>]?\d+|m)?|\sex)(?=:(?=\s*set?\s[^\n:]+:)|:(?!\s* set?\s))
  		(?:(?:\s|\s*:\s*)\w*(?:\s*=(?:[^\n\\\s]|\\.)*)?)*[\s:]
  		(?:filetype|ft|syntax)\s*=
  			vim
  		(?=\s|:|$)

  		|

  		# Emacs modeline, assuming a major mode for VimScript even exists
  		-\*-(?:\s*(?=[^:;\s]+\s*-\*-)|(?:.*?[;\s]|(?<=-\*-))mode\s*:\s*)
  			(?:Vim|VimL|VimScript)
  		(?=[\s;]|(?<![-*])-\*-).*?-\*-
  	)
scope: source.viml
contexts:
  main:
    - include: vimTodo
    - include: comments
    - include: modelines
    - include: pathname
    - include: escape
    - include: strings
    - include: hashbang
    - include: numbers
    - include: syntax
    - include: highlightLink
    - include: funcDef
    - include: augGroup
    - include: parameter
    - include: assignment
    - include: expr
    - include: keyword
    - include: register
    - include: variable
    - include: supportType
    - include: supportVariable
    - include: extraVimFunc
    - include: keywordLists
  assignment:
    - match: "[-+.]="
      scope: keyword.operator.assignment.compound.viml
    - match: "="
      scope: keyword.operator.assignment.viml
  augGroup:
    - match: '\b(aug|augroup)\b\s*([\w#]+)'
      captures:
        1: storage.function.viml
        2: entity.name.function.viml
      push:
        - meta_scope: meta.define-$1.viml
        - match: \b(\1)\s+(END)\b
          captures:
            1: storage.function.viml
            2: keyword.control.end
          pop: true
        - include: main
  commentInnards:
    - include: modelines
  comments:
    - match: '^\s*(")(?=(\s*[A-Z]\w+)+:)'
      captures:
        1: punctuation.definition.comment.viml
      push:
        - meta_scope: comment.line.quotes.viml
        - meta_content_scope: support.constant.field.viml
        - match: ((:))(.*)$
          captures:
            1: support.constant.field.viml
            2: punctuation.separator.key-value.colon.viml
          pop: true
    - match: ^\s*(")
      captures:
        1: punctuation.definition.comment.viml
      push:
        - meta_scope: comment.line.quotes.viml
        - match: $
          pop: true
        - include: commentInnards
    - match: '(?<!^)\s*(")(?=[^\n"]*$)'
      captures:
        1: punctuation.definition.comment.viml
      push:
        - meta_scope: comment.inline.quotes.viml
        - match: $
          pop: true
        - include: commentInnards
  escape:
    - match: (\\).
      scope: constant.character.escape.backslash.viml
      captures:
        1: punctuation.definition.escape.backslash.viml
  expr:
    - match: '[&|=]{2}[?#]?|[!><]=[#?]?|[=!]~(?!\/)[#?]?|[><][#?*]|\b(?:isnot|is)\b|\\|[-+%*]'
      scope: keyword.operator.logical.viml
    - match: '\s[><]\s'
      scope: keyword.operator.logical.viml
    - match: (?<=\S)!
      scope: storage.modifier.force.viml
    - match: '!(?=\S)'
      scope: keyword.operator.logical.not.viml
    - match: "{"
      scope: punctuation.expression.bracket.curly.begin.viml
    - match: "}"
      scope: punctuation.expression.bracket.curly.end.viml
    - match: '\['
      scope: punctuation.expression.bracket.square.begin.viml
    - match: '\]'
      scope: punctuation.expression.bracket.square.end.viml
    - match: \(
      scope: punctuation.expression.bracket.round.begin.viml
    - match: \)
      scope: punctuation.expression.bracket.round.end.viml
    - match: \|
      scope: punctuation.separator.statement.viml
    - match: ","
      scope: punctuation.separator.comma.viml
    - match: ":"
      scope: punctuation.separator.colon.viml
    - match: '\.{3}'
      scope: keyword.operator.rest.viml
    - match: \.
      scope: punctuation.delimiter.property.dot.viml
    - match: '&(?=\w+)'
      scope: punctuation.definition.option.viml
  extraVimFunc:
    - match: |-
        (?x)\b
        	((?:echo(?:hl?)?)|exe(?:c(?:ute)?)?|[lxvn]?noremap|smapc(?:lear)?|mapmode|xmap|(?:[xs]un|snore)map
        	|Plugin|autocmd|[cinvo]?(?:un|nore)?(?:map|menu)|(?:range)?go(?:to)?|(?:count)?(?:pop?|tag?|tn(?:ext)?|tp(?:revious)?|tr(?:ewind)?)
        	|(?:range)?(?:s(?:ubstitute)?|ret(?:ab)?|g(?:lobal)?)|unm(?:ap)?|map_l|mapc(?:lear)?|N?buffer|N?bnext|N?bNext|N?bprevious|N?bmod
        	|ab(?:breviate)?|norea(?:bbrev)?|[ic](?:un|nore)?ab|split_f|rangefold|[ic](?:un|nore)?ab|[ic]abbrev|edit_f|next_f|[vcoxli]u
        	|(?:range)?(?:w(?:rite)?|up(?:date)?)|sar|cno|[vl]n|[io]?no|[xn]n|snor|lm(?:ap)?|lunmap|lear|[vnx]m|om|[ci]m|ap|nun|sunm)
        \b
      scope: support.function.viml
  funcDef:
    - match: \b(fu(n|nction)?|end(f|fu|fun|function)?)\b
      scope: storage.function.viml
    - match: '(?:([sSgGbBwWtTlL]?(:))?[\w#]+)(?=\()'
      scope: entity.name.function.viml
      captures:
        1: storage.modifier.scope.viml
        2: punctuation.definition.scope.key-value.viml
    - match: (?<=\)|\s)(abort|dict|closure|range)(?=\s|$)
      scope: storage.modifier.$1.function.viml
  hashbang:
    - match: \A#!
      captures:
        0: punctuation.definition.comment.shebang.viml
      push:
        - meta_scope: comment.line.shebang.viml
        - match: $
          pop: true
  highlightLink:
    - match: '(?x)^\s* (:)? \s*              (?# 1: punctuation.separator.key-value.colon.viml) (hi|highlight)         (?# 2: support.function.highlight.viml) (!)?                   (?# 3: storage.modifier.force.viml) (?:\s+(def|default))? (?# 4: support.function.highlight-default.viml) (?:\s+(link))         (?# 5: support.function.highlight-link.viml) (?:\s+([-\w]+))      (?# 6: variable.parameter.group-name.viml) (?:\s+(?:(NONE)|([-\w]+)))?'
      captures:
        1: punctuation.separator.key-value.colon.viml
        2: support.function.highlight.viml
        3: storage.modifier.force.viml
        4: support.function.highlight-default.viml
        5: support.function.highlight-link.viml
        6: variable.parameter.group-name.viml
        7: support.constant.highlighting.viml
        8: variable.parameter.group-name.viml
  keyword:
    - match: \b(if|while|for|return|try|catch|finally|end(if|for|while|try)?|else(if)?|do|in|:)\b
      scope: keyword.control.$1.viml
    - match: \b(unlet)\b
      scope: keyword.operator.$1.viml
    - match: \blet\b
      scope: storage.type.let.viml
    - match: (?<=^|\n)UseVimball(?=\s*$)
      scope: support.constant.vimball.use.viml
  keywordLists:
    - include: vimTodo
    - include: vimAugroupKey
    - include: vimAutoEvent
    - include: vimBehaveModel
    - include: vimCommand
    - include: vimFTCmd
    - include: vimFTOption
    - include: vimFgBgAttrib
    - include: vimFuncKey
    - include: vimFuncName
    - include: vimGroup
    - include: vimGroupSpecial
    - include: vimHLGroup
    - include: vimHiAttrib
    - include: vimHiClear
    - include: vimHiCtermColor
    - include: vimMapModKey
    - include: vimOption
    - include: vimPattern
    - include: vimStdPlugin
    - include: vimSynCase
    - include: vimSynType
    - include: vimSyncC
    - include: vimSyncLinecont
    - include: vimSyncMatch
    - include: vimSyncNone
    - include: vimSyncRegion
    - include: vimUserAttrbCmplt
    - include: vimUserAttrbKey
    - include: vimUserCommand
    - include: vimErrSetting
  modelines:
    - match: '(?:(?:\s|^)vi(?:m[<=>]?\d+|m)?|[\t\x20]ex):\s*(?=set?\s)'
      push:
        - meta_scope: string.other.modeline.viml
        - match: ":|$"
          pop: true
        - include: main
    - match: '(?:(?:\s|^)vi(?:m[<=>]?\d+|m)?|[\t\x20]ex):'
      push:
        - meta_scope: string.other.modeline.viml
        - match: $
          pop: true
        - include: main
  numbers:
    - match: "0[xX][0-9A-Fa-f]+"
      scope: constant.numeric.hex.viml
    - match: '(?<!\w)-?\d+\.\d+[eE][-+]?\d+'
      scope: constant.numeric.float.exponential.viml
    - match: (?<!\w)-?\d+\.\d+
      scope: constant.numeric.float.viml
    - match: (?<!\w)-?\d+
      scope: constant.numeric.integer.viml
  parameter:
    - match: (-)(\w+)(=)
      scope: meta.parameter.viml
      captures:
        1: punctuation.definition.parameter.viml
        2: entity.name.parameter.viml
        3: punctuation.assignment.parameter.viml
  pathname:
    - match: ~/
      push:
        - meta_scope: constant.pathname.viml
        - match: (?=\s)
          pop: true
  regex:
    - match: (?<=\s|=)(\S)
      captures:
        1: punctuation.definition.string.begin.viml
      push:
        - meta_scope: string.regexp.viml
        - match: $|(\1)(\S*)
          captures:
            1: punctuation.definition.string.end.viml
          pop: true
        - include: regexInnards
  regexInnards:
    - match: '\['
      captures:
        0: punctuation.definition.character-class.begin.viml
      push:
        - match: '\]|$'
          captures:
            0: punctuation.definition.character-class.end.viml
          pop: true
        - include: regexInnards
    - match: (\\).
      scope: constant.character.escape.viml
      captures:
        1: punctuation.definition.backslash.escape.viml
  regexOffset:
    - match: '(ms|me|hs|he|rs|re|lc)(=)(?:(\d+)|([se])(?:([-+])(\d+))?)(,)?'
      scope: meta.pattern-offset.viml
      captures:
        1: constant.language.pattern-offset.viml
        2: punctuation.assignment.parameter.viml
        3: constant.numeric.integer.viml
        4: constant.language.pattern-position.viml
        5: keyword.operator.arithmetic.viml
        6: constant.numeric.integer.viml
        7: punctuation.separator.comma.viml
  register:
    - match: '(@)([-"A-Za-z\d:.%#=*+~_/])'
      scope: variable.other.register.viml
      captures:
        1: punctuation.definition.register.viml
  strings:
    - match: (")(")
      scope: string.quoted.double.empty.viml
      captures:
        1: punctuation.definition.string.begin.viml
        2: punctuation.definition.string.end.viml
    - match: (')(')
      scope: string.quoted.single.empty.viml
      captures:
        1: punctuation.definition.string.begin.viml
        2: punctuation.definition.string.end.viml
    - match: '(")((?:[^\\"]|\\.)*)(")'
      scope: string.quoted.double.viml
      captures:
        1: punctuation.definition.string.begin.viml
        3: punctuation.definition.string.end.viml
    - match: "(')((?:[^']|'')*)(')"
      scope: string.quoted.single.viml
      captures:
        1: punctuation.definition.string.begin.viml
        3: punctuation.definition.string.end.viml
    - match: '(/)(?:\\\\|\\/|[^\n/])*(/)'
      scope: string.regexp.interpolated.viml
      captures:
        1: punctuation.section.regexp.begin.viml
        2: punctuation.section.regexp.end.viml
  supportType:
    - match: (<).*?(>)
      scope: support.type.viml
      captures:
        1: punctuation.definition.bracket.angle.begin.viml
        2: punctuation.definition.bracket.angle.end.viml
  supportVariable:
    - match: '\b(?:am(?:enu)?|(?:hl|inc)?search|[Bb]uf(?:[Nn]ew[Ff]ile|[Rr]ead)?|[Ff]ile[Tt]ype)\b'
      scope: support.variable.viml
  syntax:
    - match: ^\s*(:)?\s*(syntax|syn?)(?=\s|$)
      captures:
        1: punctuation.separator.key-value.colon.viml
        2: support.function.syntax-item.viml
      push:
        - meta_scope: meta.syntax-item.viml
        - match: $
          pop: true
        - match: \G\s+(case)(?:\s+(match|ignore))?(?=\s|$)
          captures:
            1: support.function.syntax-case.viml
            2: support.constant.$2-case.viml
        - match: \G\s+(spell)(?:\s+(toplevel|notoplevel|default))?(?=\s|$)
          captures:
            1: support.function.syntax-spellcheck.viml
            2: support.constant.$2-checking.viml
        - match: '\G\s+(keyword)(?:\s+([-\w]+))?'
          captures:
            1: support.function.syntax-keywords.viml
            2: variable.parameter.group-name.viml
          push:
            - meta_content_scope: keyword.other.syntax-definition.viml
            - match: (?=$)
              pop: true
            - include: syntaxOptions
            - include: assignment
            - include: expr
        - match: '\G\s+(match)(?:\s+([-\w]+))?\s*'
          captures:
            1: support.function.syntax-match.viml
            2: variable.parameter.group-name.viml
          push:
            - match: (?=$)
              pop: true
            - include: syntaxRegex
        - match: '\G\s+(region)(?:\s+([-\w]+))?'
          captures:
            1: support.function.syntax-region.viml
            2: variable.parameter.group-name.viml
          push:
            - match: (?=$)
              pop: true
            - include: syntaxOptions
            - include: main
        - match: '\G\s+(cluster)(?:\s+([-\w]+))?(?=\s|$)'
          captures:
            1: support.function.syntax-cluster.viml
            2: variable.parameter.group-name.viml
          push:
            - match: (?=$)
              pop: true
            - include: syntaxOptions
            - include: main
        - match: \G\s+(conceal)(?:\s+(on|off)(?=\s|$))?
          captures:
            1: support.function.syntax-conceal.viml
            2: support.constant.boolean.$2.viml
        - match: '\G\s+(include)(?:\s+((@)?[-\w]+))?(?:\s+(\S+))?'
          captures:
            1: support.function.syntax-include.viml
            2: variable.parameter.group-name.viml
            3: punctuation.definition.group-reference.viml
            4: string.unquoted.filename.viml
        - match: \G\s+(sync)(?=\s|$)
          captures:
            1: support.function.syntax-sync.viml
          push:
            - match: $
              pop: true
            - match: \G\s+(fromstart)(?=\s|$)
              captures:
                1: support.constant.sync-$1.viml
            - match: '\G\s+(ccomment|clear)(?:\s+(?![-\w]+\s*=)([-\w]+))?'
              captures:
                1: support.constant.sync-$1.viml
                2: variable.parameter.group-name.viml
            - match: \G\s+(minlines|lines)\s*(=)(\d*)
              captures:
                1: support.constant.sync-mode.viml
                2: punctuation.assignment.parameter.viml
                3: constant.numeric.integer.viml
            - match: '(?x)\G\s+(match|region)(?:\s+(?![-\w]+\s*=)([-\w]+))?'
              captures:
                1: support.constant.sync-mode.viml
                2: variable.parameter.group-name.viml
                3: support.constant.sync-mode-location.viml
            - match: '(?<=\s)(groupt?here|linecont)(?:\s+(?![-\w]+\s*=)([-\w]+))?(?=\s|$)'
              captures:
                1: support.constant.sync-match.viml
                2: variable.parameter.group-name.viml
              push:
                - match: (?=$)
                  pop: true
                - include: syntaxRegex
            - include: syntaxOptions
        - include: main
  syntaxOptions:
    - match: (?<=\s)(start|skip|end)(?:\s*(=))
      captures:
        1: support.constant.$1-pattern.viml
        2: punctuation.assignment.parameter.viml
      push:
        - meta_scope: meta.syntax-item.pattern-argument.viml
        - match: (?=$|\s)
          pop: true
        - include: regex
    - match: |-
        (?x)(?<=\s)
        ((?:matchgroup|contains|containedin|nextgroup|add|remove|minlines|linebreaks|maxlines)(?=\s*=)
        |(?:cchar|conceal|concealends|contained|display|excludenl|extend|fold|keepend|oneline|skipempty|skipnl|skipwhite|transparent))
        (?:(?=$|\s)|\s*(=)(\S*)?)
      scope: meta.syntax-item.argument.viml
      captures:
        1: support.constant.syntax-$1.viml
        2: punctuation.assignment.parameter.viml
        3: string.unquoted.syntax-option.viml
  syntaxRegex:
    - include: syntaxOptions
    - match: (?<=\s)(\S)
      captures:
        1: punctuation.definition.string.begin.viml
      push:
        - meta_scope: string.regexp.viml
        - match: (?:(\1)(\S*)(.*))?$
          captures:
            1: punctuation.definition.string.end.viml
          pop: true
        - include: regexInnards
  variable:
    - match: \b(self)\b
      scope: variable.language.self.viml
    - match: (\$)\w+
      scope: support.variable.environment.viml
      captures:
        1: punctuation.definition.variable.viml
    - match: '(&?)(?:([sSgGbBwWlLaAvV](:))|[@$]|&(?!&))\w*'
      scope: variable.other.viml
      captures:
        1: punctuation.definition.reference.viml
        2: storage.modifier.scope.viml
        3: punctuation.definition.scope.key-value.viml
  vimAugroupKey:
    - match: |-
        (?x) \b
        ( aug
        | augroup
        ) \b
      scope: support.function.vimAugroupKey.viml
  vimAutoEvent:
    - match: |-
        (?xi) \b
        ( BufAdd
        | BufCreate
        | BufDelete
        | BufEnter
        | BufFilePost
        | BufFilePre
        | BufHidden
        | BufLeave
        | BufNewFile
        | BufNew
        | BufReadCmd
        | BufReadPost
        | BufReadPre
        | BufRead
        | BufUnload
        | BufWinEnter
        | BufWinLeave
        | BufWipeout
        | BufWriteCmd
        | BufWritePost
        | BufWritePre
        | BufWrite
        | CmdUndefined
        | CmdlineChanged
        | CmdlineEnter
        | CmdlineLeave
        | CmdwinEnter
        | CmdwinLeave
        | ColorSchemePre
        | ColorScheme
        | CompleteChanged
        | CompleteDone
        | CursorHoldI
        | CursorHold
        | CursorMovedI
        | CursorMoved
        | DiffUpdated
        | DirChanged
        | EncodingChanged
        | ExitPre
        | FileAppendCmd
        | FileAppendPost
        | FileAppendPre
        | FileChangedRO
        | FileChangedShellPost
        | FileChangedShell
        | FileEncoding
        | FileReadCmd
        | FileReadPost
        | FileReadPre
        | FileType
        | FileWriteCmd
        | FileWritePost
        | FileWritePre
        | FilterReadPost
        | FilterReadPre
        | FilterWritePost
        | FilterWritePre
        | FocusGained
        | FocusLost
        | FuncUndefined
        | GUIEnter
        | GUIFailed
        | InsertChange
        | InsertCharPre
        | InsertEnter
        | InsertLeave
        | MenuPopup
        | OptionSet
        | QuickFixCmdPost
        | QuickFixCmdPre
        | QuitPre
        | RemoteReply
        | SafeStateAgain
        | SafeState
        | SessionLoadPost
        | ShellCmdPost
        | ShellFilterPost
        | SourceCmd
        | SourcePost
        | SourcePre
        | SpellFileMissing
        | StdinReadPost
        | StdinReadPre
        | SwapExists
        | Syntax
        | TabClosed
        | TabEnter
        | TabLeave
        | TabNew
        | TermChanged
        | TermResponse
        | TerminalOpen
        | TerminalWinOpen
        | TextChangedI
        | TextChangedP
        | TextChanged
        | TextYankPost
        | User
        | VimEnter
        | VimLeavePre
        | VimLeave
        | VimResized
        | WinEnter
        | WinLeave
        | WinNew
        ) \b
      scope: support.function.auto-event.viml
  vimBehaveModel:
    - match: |-
        (?x) \b
        ( mswin
        | xterm
        ) \b
      scope: support.function.vimBehaveModel.viml
  vimCommand:
    - match: |-
        (?x) \b
        ( abc
        | abclear
        | abo
        | aboveleft
        | ab
        | addd
        | all?
        | ar
        | args
        | arga
        | argadd
        | argd
        | argdelete
        | argdo
        | arge
        | argedit
        | argg
        | argglobal
        | argl
        | arglocal
        | argu
        | argument
        | as
        | ascii
        | au
        | a
        | bN
        | bNext
        | b
        | buffer
        | ba
        | ball
        | badd?
        | bd
        | bdelete
        | bel
        | belowright
        | bf
        | bfirst
        | bl
        | blast
        | bm
        | bmodified
        | bn
        | bnext
        | bo
        | botright
        | bp
        | bprevious
        | br
        | brewind
        | break?
        | breaka
        | breakadd
        | breakd
        | breakdel
        | breakl
        | breaklist
        | bro
        | browse
        | bufdo
        | buffers
        | bun
        | bunload
        | bw
        | bwipeout
        | cN
        | cNext
        | cNf
        | cNfile
        | c
        | change
        | cabc
        | cabclear
        | cabo
        | cabove
        | cad
        | caddbuffer
        | cadde
        | caddexpr
        | caddf
        | caddfile
        | caf
        | cafter
        | call?
        | cat
        | catch
        | ca
        | cb
        | cbuffer
        | cbe
        | cbefore
        | cbel
        | cbelow
        | cbo
        | cbottom
        | ccl
        | cclose
        | cc
        | cdo
        | cd
        | ce
        | center
        | cex
        | cexpr
        | cf
        | cfile
        | cfdo
        | cfir
        | cfirst
        | cg
        | cgetfile
        | cgetb
        | cgetbuffer
        | cgete
        | cgetexpr
        | changes
        | chd
        | chdir
        | che
        | checkpath
        | checkt
        | checktime
        | chi
        | chistory
        | cl
        | clist
        | cla
        | clast
        | cle
        | clearjumps
        | clo
        | close
        | cmapc
        | cmapclear
        | cn
        | cnext
        | cnew
        | cnewer
        | cnf
        | cnfile
        | cnor
        | co
        | copy
        | col
        | colder
        | colo
        | colorscheme
        | comc
        | comclear
        | comp
        | compiler
        | com
        | con
        | continue
